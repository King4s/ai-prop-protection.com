{
    "manifest_version": 3,
    "name": "AI-Prop-Protection (Injection Test)",
    "version": "1.0.0-alpha",
    "description": "Diagnostic tool to test programmatic script injection.",
    "homepage_url": "https://github.com/King4s/ai-prop-protection.com",
    "author": "King4s",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["*://meta.ai/*"],
    "background": { "service_worker": "background.js" },
    "action": { "default_popup": "popup.html" },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
