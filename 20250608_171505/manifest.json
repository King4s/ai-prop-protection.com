{
    "manifest_version": 3,
    "name": "AI-Prop-Protection (Canary Test)",
    "version": "0.9.9",
    "description": "Diagnostic tool to test content script injection and messaging.",
    "homepage_url": "https://github.com/King4s/ai-prop-protection.com",
    "author": "King4s",
    "permissions": ["activeTab", "scripting"],
    "background": { "service_worker": "background.js" },
    "action": { "default_popup": "popup.html" },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://meta.ai/*"],
            "js": ["content.js"]
        }
    ]
}
